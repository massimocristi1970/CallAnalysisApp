# Supabase Configuration
# Copy this file to .env and fill in your Supabase credentials
# Get these values from your Supabase project settings: https://app.supabase.com/

# Your Supabase project URL (e.g., https://your-project.supabase.co)
SUPABASE_URL=your_supabase_url_here

# Your Supabase anon/public API key
SUPABASE_KEY=your_supabase_anon_key_here

# Optional: Use local SQLite database instead of Supabase (set to 'true' for local development)
USE_LOCAL_DB=false
